SELECT
 cq.CBS_MODULE, cq.SEVERITY, cq.PRIORITY, cq.ID, cq.TITLE
FROM CQ_RO.CBS_DEFECT cq
WHERE
 (cq.CUSTOMER_NAME='!BRANCH!')
 AND (cq.PRODUCT_NAME='Comverse ONE CV-RT-FX')
 AND (cq.SERVICE_NAME='Self Service')
 AND cq.STATE = 16778736 -- Ready4Packaging
ORDER BY cq.CBS_MODULE, cq.SEVERITY;
quit
;
